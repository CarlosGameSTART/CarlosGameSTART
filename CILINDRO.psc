Algoritmo CILINDRO
	Definir RADIO, VOLUMEN, AREA, ALTURA Como Real;
	Escribir "Altura";
	Leer ALTURA;
	Escribir "Radio";
	Leer RADIO;
	AREA<-2*PI*RADIO*ALTURA;
	VOLUMEN<-PI*RADIO^2*ALTURA;
	Escribir "Area: ", area;
	Escribir "Volumen: " volumen;
	Escribir "Carlos Alberto Cortinas";
FinAlgoritmo
